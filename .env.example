# App Store Connect API Keys
# Generate these from App Store Connect (https://appstoreconnect.apple.com/access/api)
APP_STORE_CONNECT_ISSUER_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
APP_STORE_CONNECT_API_KEY_ID=XXXXXXXXXX
# The private key content, including BEGIN and END lines
APP_STORE_CONNECT_API_PRIVATE_KEY=-----BEGIN PRIVATE KEY-----\nYOUR_KEY_HERE\n-----END PRIVATE KEY-----

# Appetize.io API Token
# Get this from your Appetize.io dashboard (https://appetize.io/docs/api)
APPETIZE_API_TOKEN=tok_xxxxxxxxxxxxxxxxxxxx

# Certificates and Provisioning
# Base64 encoded distribution certificate (.p12 file)
# Generate with: base64 -i Certificates.p12 | tr -d '\n'
BUILD_CERTIFICATE_BASE64=MIIKsAIBAzCCCngGCSqGSIb3DQEHAaCCCmkEggpl...

# Password for the .p12 file
P12_PASSWORD=your_p12_password_here

# Base64 encoded provisioning profile
# Generate with: base64 -i profile.mobileprovision | tr -d '\n'
BUILD_PROVISION_PROFILE_BASE64=MIIKsAIBAzCCCngGCSqGSIb3DQEHAaCCCmkEggpl...

# Password for the temporary keychain
KEYCHAIN_PASSWORD=temp_keychain_password

# Webhook URL for notifications (e.g., Slack)
WEBHOOK_URL=https://hooks.slack.com/services/XXX/YYY/ZZZ 